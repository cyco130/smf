{
	"compilerOptions": {
		"target": "es2022",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"types": ["vite/client"]
	}
}
